{
  "name": "sepaket-api",
  "version": "1.0.0",
  "description": "sepaket api",
  "main": "index.js",
  "scripts": {
    "test": "npm run test",
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "node_modules/.bin/eslint src --ext .js,.ts",
    "lint-fix": "node_modules/.bin/eslint --fix --ext .js,.ts src",
    "generate:migration": "node_modules/.bin/npx sequelize-cli migration:generate --name",
    "migrate": "node_modules/.bin/npx sequelize-cli db:migrate",
    "migrate:undo": "node_modules/.bin/npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "node_modules/.bin/npx sequelize-cli db:migrate:undo:all",
    "generate:seed": "node_modules/.bin/npx sequelize-cli seed:generate --name",
    "seed": "node_modules/.bin/npx sequelize-cli db:seed:all",
    "seed:undo": "node_modules/.bin/npx sequelize-cli db:seed:undo",
    "seed:undo:all": "node_modules/.bin/npx sequelize-cli db:seed:undo:all",
    "reset:db": "npm run migrate:undo:all && npm run migrate && npm run seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node_modules/.bin/eslint --fix --ext .js,.ts, src --max-warnings=0"
    }
  },
  "keywords": [
    "express",
    "postgre",
    "sequelize",
    "expedition"
  ],
  "author": "sepaket.co.id",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "express": "^4.18.1",
    "express-group-routes": "^1.1.0",
    "helmet": "^5.1.0",
    "http-errors": "^2.0.0",
    "husky": "^3.1.0",
    "joi": "^17.6.0",
    "nodemailer": "^6.7.5",
    "nodemon": "^2.0.16",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "sequelize-cli": "^6.4.1"
  }
}
